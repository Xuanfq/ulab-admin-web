    "@iconify-icons/ep": "^1.2.12",
    "@iconify-icons/ri": "^1.2.10",
    "@iconify/vue": "^4.1.2",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@pureadmin/theme": "^3.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/gradient-string": "^1.1.6",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.14.8",
    "@types/nprogress": "0.2.3",
