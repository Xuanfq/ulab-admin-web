    "dayjs": "^1.11.11",
    "echarts": "^5.5.0",
    "element-plus": "^2.7.6",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.1.7",
    "pinyin-pro": "^3.22.2",
    "plus-pro-components": "0.1.11",
    "qrcode": "^1.5.3",
    "qs": "^6.12.1",
    "responsive-storage": "^2.2.0",
    "sortablejs": "^1.15.2",
    "typeit": "^8.8.3",
    "version-rocket": "^1.7.1",
    "vue": "^3.4.30",
